{
    "name": "Mikkellindblom/utilities",
    "description": "Transform your PHP to JavaScript",
    "keywords": ["laravel", "javascript"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mikkel lindblom",
            "email": "mikkel.lindblom@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Mikkellindblom\\Utilities\\JavaScript\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mikkellindblom\\Utilities\\JavaScript\\JavaScriptServiceProvider"
            ],
            "aliases": {
                "JavaScript": "Mikkellindblom\\Utilities\\JavaScript\\JavaScriptFacade"
            }
        }
    }
}
